{
  "name": "@chr33s/unicode-properties",
  "version": "0.0.1",
  "description": "Provides fast access to unicode character properties",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "npm run generate && tsc",
    "clean": "rm -rd {dist,src/*.json}",
    "generate": "node scripts/generate.ts",
    "test": "vitest run",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "base64-arraybuffer": "^1.0.2",
    "pako": "^2.1.0",
    "unicode-trie": "^2.0.0"
  },
  "devDependencies": {
    "@types/node": "^24.10.1",
    "@types/pako": "^2.0.4",
    "@types/unicode-trie": "^2.0.3",
    "codepoints": "^1.3.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.12"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/Hopding/unicode-properties.git"
  },
  "keywords": [
    "unicode",
    "metadata",
    "character",
    "codepoint"
  ],
  "author": "Andrew Dillon <andrew.dillon.j@gmail.com>",
  "contributors": [
    "Devon Govett <devongovett@gmail.com> (http://badassjs.com/)"
  ],
  "bugs": {
    "url": "https://github.com/Hopding/unicode-properties/issues"
  },
  "license": "MIT",
  "homepage": "https://github.com/Hopding/unicode-properties"
