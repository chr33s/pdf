  "name": "@chr33s/pdf-lib",
  "version": "2.5.3",
  "description": "Create and modify PDF files with JavaScript",
  "author": "Andrew Dillon <andrew.dillon.j@gmail.com>",
  "type": "module",
  "packageManager": "npm@10.9.3+",
  "contributors": [
